{
  "name": "zaity",
  "version": "1.0.0-beta.1",
  "description": "",
  "exports": {
    ".": {
      "import": "./src/esm/browser.esm.js",
      "require": "./dist/cjs/browser.cjs.js",
      "types": "./dist/types/browser/index.d.ts"
    },
    "./node": {
      "import": "./dist/esm/node.esm.js",
      "require": "./dist/cjs/node.cjs.js",
      "types": "./dist/types/node/index.d.ts"
    },
    "./middlewares": {
      "import": "./dist/esm/middlewares.esm.js",
      "require": "./dist/cjs/middlewares.cjs.js",
      "types": "./dist/types/middlewares/index.d.ts"
    }
  },
  "files": [
    "/lib",
    "README.md",
    "LICENSE",
    "logolibrary.scg"
  ],
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack --config webpack.config.js --mode development",
    "build:prod": "webpack --config webpack.config.js --mode production && npm run types",
    "commit": "git-cz",
    "prepare": "husky install",
    "postinstall": "npx husky install",
    "docs": "jsdoc -c jsdoc.config.json",
    "types": "tsc --allowJs -d --emitDeclarationOnly",
    "ci": "npm pkg delete scripts.prepare & npm ci --omit=dev",
    "semantic-release": "semantic-release"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.2",
    "detect-node": "^2.1.0",
    "execa": "^7.1.1",
    "figlet": "^1.6.0",
    "inquirer": "^9.2.8"
  },
  "peerDependencies": {
    "@aws-sdk/client-transcribe": "^3.378.0",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@aws-sdk/client-transcribe": "^3.382.0",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.0.0",
    "@semantic-release/github": "^9.2.1",
    "@semantic-release/npm": "^11.0.0",
    "@semantic-release/release-notes-generator": "^12.0.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dts-bundle-webpack": "^1.0.2",
    "dts-webpack-plugin": "^0.0.9",
    "husky": "^8.0.0",
    "jsdoc": "^4.0.2",
    "npm-dts-webpack-plugin": "^1.3.12",
    "semantic-release": "^22.0.5",
    "socket.io-client": "^4.7.2",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "~5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
